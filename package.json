{
  "name": "minestack",
  "version": "1.0.0",
  "description": "",
  "private": "true",
  "main": "index.js",
  "scripts": {
    "setup": "yarn && lerna bootstrap",
    "setup:clean": "rimraf ./node_modules ./packages/**/node_modules && yarn setup",
    "build": "rimraf ./dist && yarn lerna run build",
    "lint:staged": "lint-staged",
    "lint:fix": "eslint --config ./.eslintrc.js --fix",
    "test:coverage": "rimraf ./coverage /vagrant/coverage && yarn lerna run test:coverage && chmod +x ./coverage.sh && ./coverage.sh && cp -r coverage/ /vagrant/",
    "test:coverage:win": "rimraf ./coverage && yarn lerna run test:coverage && ./coverage.sh",
    "prj-g": "scaffdog generate --project ./.scaffdog && yarn lint:fix ./ && yarn setup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MineStackPrj/MineStack.git"
  },
  "keywords": [],
  "author": "t-akasaki <t.akasaki.csharp@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MineStackPrj/MineStack/issues"
  },
  "homepage": "https://github.com/MineStackPrj/MineStack#readme",
  "workspaces": {
    "packages": [
      "packages/app/*",
      "packages/common/*",
      "packages/environment/*",
      "packages/client/*",
      "packages/server/*"
    ],
    "nohoist": []
  },
  "dependencies": {},
  "devDependencies": {
    "@stoplight/prism-cli": "^4.1.2",
    "@types/jest": "26.0.19",
    "@types/node": "14.14.13",
    "commitlint": "11.0.0",
    "conventional-changelog-conventionalcommits": "4.5.0",
    "cross-conf-env": "1.2.1",
    "cross-env": "^7.0.3",
    "eslint": "7.15.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.9.2",
    "eslint-webpack-plugin": "2.1.0",
    "husky": "4.3.5",
    "istanbul": "0.4.5",
    "istanbul-merge": "1.1.1",
    "lcov-result-merger": "^3.1.0",
    "lerna": "3.22.1",
    "lint-staged": "10.5.3",
    "prettier": "2.1.2",
    "rimraf": "^3.0.2",
    "scaffdog": "^1.0.0"
  }
}
